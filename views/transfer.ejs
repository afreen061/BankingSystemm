            <th>S.No</th>
            <th>From</th>
            <th>To</th>
            <th>Amount</th>
            
            
        </tr>
        
        <%
        if(transdata.length!=0){
        var i=1;
        transdata.forEach(function(rows){
        %>
        <tr class="bbody">
            <td><%=i; %></td>
            
            <td><%=rows.From %></td>
            <td><%=rows.to %></td>
            <td><%=rows.Amount %></td>
            
        
            
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
